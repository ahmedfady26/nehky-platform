    "lucide-react": "^0.525.0",
    "next": "^15.3.4",
    "next-auth": "^4.24.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.22.4"
